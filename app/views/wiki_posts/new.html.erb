<h1>New wiki post</h1>

<%= form_with model: @wiki_post, local: true do |form| %>

  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.submit "Create" %>
  </div>

<% end %>

<br>

<div>
  <%= link_to "Back to wiki posts", wiki_posts_path %>
</div>
